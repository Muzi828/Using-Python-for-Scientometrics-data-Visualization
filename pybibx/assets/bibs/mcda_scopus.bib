@ARTICLE{Mohsin2022,
author={Mohsin, M. and Hengbin, Y. and Luyao, Z. and Rui, L. and Chong, Q. and Mehak, A.},
title={An Application of Multiple-Criteria Decision Analysis for Risk Prioritization and Management: A Case Study of the Fisheries Sector in Pakistan},
journal={Sustainability (Switzerland)},
year={2022},
volume={14},
number={14},
doi={10.3390/su14148831},
art_number={8831},
note={cited By 0},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85136396077&doi=10.3390%2fsu14148831&partnerID=40&md5=3a4369521d7fe5541ecef9fc96ea522c},
affiliation={College of International Finance and Trade, Zhejiang YueXiu University of Foreign Languages, Shaoxing, 312000, China; School of Finance and Trade, Wenzhou Business College, Wenzhou, 325035, China; School of Business, Hanyang University, Seoul, 04763, South Korea; College of Fisheries, Ocean University of China, Qingdao, 266003, China},
abstract={In Pakistan, the fisheries sector is capable of making a significant contribution to the national economy. However, the proper and sustainable development of this sector is essential to its success, and we need to be aware of all the risks that it faces. At present, there is a dearth of comprehensive research that details, compares, and proposes applied measures to mitigate the risks facing the fisheries sector. Thus, this study is the first novel attempt to fill this gap. The data were collected through a survey and analyzed by multi-criteria decision analysis (MCDA). The study postulates that Sindh fisheries are affected by five main risk factors, namely technical, market, ecological, natural, and management. These risk factors are arranged from least to most significant. With regard to the performances of the main risk factors, management risk was ranked as the greatest risk, followed by ecological risk, natural risk, and technical risk. The findings of this study provide a road map for managerial decisions. Furthermore, this study also presents some potential limitations related to the scale of the data and analysis methods. Future studies may therefore use data collected on a large scale and alternative quantitative approaches. © 2022 by the authors.},
