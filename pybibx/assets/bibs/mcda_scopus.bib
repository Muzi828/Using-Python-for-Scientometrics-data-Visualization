@ARTICLE{Poehler2022,
author={Poehler, D. and Czerniecki, J. and Norvell, D. and Henderson, A. and Dolan, J. and Devine, B.},
title={The Development and Pilot Study of a Multiple Criteria Decision Analysis (MCDA) to Compare Patient and Provider Priorities around Amputation-Level Outcomes},
journal={MDM Policy and Practice},
year={2022},
