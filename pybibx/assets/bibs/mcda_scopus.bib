@ARTICLE{Janošovský2022775,
author={Janošovský, J. and Boháciková, V. and Rosa, I. and Vincent, G. and Labovská, Z. and Jelemenský, L.},
title={Recognition of Process Safety Position in Multiple-criteria Decision Analysis in Process Design},
