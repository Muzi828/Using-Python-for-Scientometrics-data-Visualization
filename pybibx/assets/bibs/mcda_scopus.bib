@CONFERENCE{Pizon2019,
author={Pizon, J. and Lazniewska-Piekarczyk, B.},
title={Comparison of Efficiency of Accelerating Admixtures for Concrete Using Multiple-Criteria Decision Analysis (MCDA)},
journal={IOP Conference Series: Materials Science and Engineering},
year={2019},
volume={471},
number={3},
doi={10.1088/1757-899X/471/3/032043},
art_number={032043},
note={cited By 1; Conference of 3rd World Multidisciplinary Civil Engineering, Architecture, Urban Planning Symposium, WMCAUS 2018 ; Conference Date: 18 June 2018 Through 22 June 2018;  Conference Code:145505},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85062459786&doi=10.1088%2f1757-899X%2f471%2f3%2f032043&partnerID=40&md5=a006dc7dfcbb7e25da34c8badba7e0a4},
affiliation={Silesian University of Technology, Faculty of Civil Engineering, Akademicka 5, Gliwice, Poland},
abstract={A lot of different types of accelerating admixtures for concrete are used nowadays. The aim of this paper is to determine efficiency relations between several accelerating admixtures for concrete with usage of multiple-criteria decision analysis (MCDA). Tests of cements' initial setting time, consistency and compressive strength of mortars were made. Tests of cements and fresh mortars were conducted in temperature 8°C and 20°C. Compressive strength were tested after 12, 24, 48 hours and 7, 28 days of curing. Specimens for those tests were cured in temperature 8°C in water and 20°C in climatic chamber with relative humidity 60%. Mortars were made of Portland cement (CEM I 52,5R), Portland-slag cement (CEM II/B-S) and two kinds of blast furnace cement (CEM III/A, CEM III/C). Examined admixtures were based on calcium formate, crystal seeds and calcium nitrate. All of them are described as both set and hardening accelerators for concrete. Admixtures were added in maximum dosage allowed by producer. All of admixtures caused decrease of initial setting time and increase of early compressive strength (12-48 hours) with varying degrees. Decrease of 28 days compressive strength was not noted in case of any admixture. In order to determination of efficiency relations the multiple-criteria decision analysis (MCDA) was made for every kind of cement separately. Criteria were defined on the basis of conducted tests and were divided accordingly to environmental conditions. Weights of criteria were evaluated with pair analysis method. Values of tests' results were converted with usage of Neuman-Morgenstern method. Result of analysis is sum of criteria evaluations according to their weights. Accelerating admixture based on crystal seeds in proven to be the most efficient one, while the one based on calcium formate is the least efficient of tested admixtures. © 2019 Institute of Physics Publishing. All rights reserved.},
keywords={Blast furnaces;  Calcium compounds;  Compressive strength;  Computational electromagnetics;  Concretes;  Curing;  Decision making;  Decision theory;  Efficiency;  Mortar;  Operations research;  Portland cement;  Slag cement;  Slags;  Urban planning, Accelerating admixtures;  Blast-furnace cements;  Calcium nitrate;  Climatic chamber;  Environmental conditions;  Initial setting time;  Multiple criteria decision analysis;  Portland slag cements, Concrete additives},
references={Neville, A.M., (2011) Properties of Concrete, , (Harlow: Pearson); Ramachandran, V.S., (1995) Concrete Admixtures Handbook, , (Park Ridge: Noyes Publications); Kurdowski, W., (2014) Cement and Concrete Chemistry, , (Netherlands: Springer); Reichenbach-Klinke, R., Nicoleau, L., Use of CSH suspension in well cementing (2011) US Patent, , App. 13/082,483; Dittmar, S., Hauck, H., Konig, R., C-S-H: A state-of-the-art concept to accelerate concrete hardening: Part i (2013) BFT International, 1; Justnes, H., Ng, S., Hardening accelerators for fly ash blended cement (2015) SINTEF Building and Infrastructure - COIN, , Project report 71; Quentin, F., MC gives concrete a FastKick! (2016) MCaktiv, 3, p. 13; Siddique, R., (2011) Supplementary Cementing Materials, 37. , (Berlin Heidelberg: Springer-Verlag)},
