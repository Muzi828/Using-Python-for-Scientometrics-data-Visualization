@CONFERENCE{Lambert20102696,
author={Lambert, J.H. and Karvetski, C.W. and Linkov, I. and Abdallah, T.},
title={Energy security of military and industrial facilities: A scenario-based multiple criteria decision analysis to identify threats and opportunities},
journal={10th International Conference on Probabilistic Safety Assessment and Management 2010, PSAM 2010},
year={2010},
volume={4},
pages={2696-2704},
note={cited By 3; Conference of 10th International Conference on Probabilistic Safety Assessment and Management 2010, PSAM 2010 ; Conference Date: 7 June 2010 Through 11 June 2010;  Conference Code:95447},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-84873605693&partnerID=40&md5=be08d839664554e9d3cb790db4a161d0},
affiliation={University of Virginia, Charlottesville, VA, United States; Engineer Research and Development Center, US Army Corps. of Engineers, United States},
abstract={Energy security is the capacity to avoid adverse impact of disruptions from natural, accidental, or intentional events affecting energy and utility supply and distributions systems [1]. The integration of multiple fuel sources, use of regional renewable energies, consideration of cultural acceptance, defined criteria and metrics, and appropriate research and development all constitute a plan for achieving energy security [2]. When evaluating alternative investment strategies in energy security for military and industrial facilities, it is important to consider future scenarios of the energy environment. Some scenarios may be evidence-based projections derived from geographic, regulatory, geopolitical, and other driving forces. Other scenarios could reflect the advocacy positions of various stakeholders in a large-scale infrastructure or military/industrial system. This paper reflects a philosophy to incorporate uncertain scenarios to update a scoring function of a multiple criteria decision analysis (MCDA). The approach identifies what scenarios most affect the prioritization of investment portfolios, and what portfolios are most opportunistic or vulnerable across the scenarios. The approach efficiently adjusts the scoring function and is therefore suitable for considering a potentially large set of scenarios in stakeholder negotiation, group decision making, and brainstorming.},
