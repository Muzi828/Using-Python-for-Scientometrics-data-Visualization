@ARTICLE{Mokarram20202251,
author={Mokarram, M. and Ghasemi, M.M. and Zarei, A.R.},
title={Evaluation of the soil fertility for corn production (Zea Mays) using the multiple-criteria decision analysis (MCDA)},
