@article{ WOS:000289819400010,
Author = {Lienert, Judit and Koller, Mirjam and Konrad, Jonas and McArdell,
   Christa S. and Schuwirth, Nele},
Title = {Multiple-Criteria Decision Analysis Reveals High Stakeholder Preference
   to Remove Pharmaceuticals from Hospital Wastewater},
