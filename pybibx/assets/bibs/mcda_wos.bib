Author = {Lee, Wah Ching and Hung, Faan Hei and Tsang, Kim Fung and Tung, Hoi
   Ching and Lau, Wing Hong and Rakocevic, Veselin and Lai, Loi Lei},
Title = {A Speedy Cardiovascular Diseases Classifier Using Multiple Criteria
   Decision Analysis},
