Title = {Mathematic models based on multiple-criteria decision analysis for
   tuning industrial CNN in an FPGA computing cluster},
Booktitle = {PROCEEDINGS OF THE 2020 31ST INTERNATIONAL WORKSHOP ON RAPID SYSTEM
   PROTOTYPING (RSP): SHORTENING THE PATH FROM SPECIFICATION TO PROTOTYPE:
   SHORTENING THE PATH FROM SPECIFICATION TO PROTOTYPE},
