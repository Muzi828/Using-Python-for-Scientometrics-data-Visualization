@article{ WOS:000664458800001,
Author = {Inusah, Abdulai and Turunen, Esko},
Title = {A Paraconsistent Pavelka Technique for Multiple Criteria Decision
   Analysis},
Journal = {JOURNAL OF MULTIPLE-VALUED LOGIC AND SOFT COMPUTING},
Year = {2021},
Volume = {37},
Number = {1-2},
Pages = {1-25},
Abstract = {Decision making involving multiple alternatives or strategies in respect
   of a number of conflicting criteria is usually a daunting problem for
   practitioners from across diverse fields. As a result, academics,
   particularly decision science researchers have been working so hard over
   the years to develop appropriate multicriteria decision making methods
   to aid decision makers in their quest to establish the optimum decisions
   in their respective fields and problems. In this paper, a technique for
   decision making in environments characterised by the availability of
   incompletely ignorant facts, partially conflicting and vague information
   is proposed. The technique is dependent on paraconsistent logic, Pavelka
   style fuzzy logic and fuzzy similarity relations. To demonstrate the use
   of the technique, it is applied to the selection of the best or optimal
   five energy mix from eight electricity generation sources for Ghana.},
