Title = {Multiple Criteria Decision Analysis for Health Technology Assessment of
   Medical Devices: A Winning Hospital-Based Experience},
Booktitle = {XV MEDITERRANEAN CONFERENCE ON MEDICAL AND BIOLOGICAL ENGINEERING AND
   COMPUTING - MEDICON 2019},
