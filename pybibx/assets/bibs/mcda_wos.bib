@inproceedings{ WOS:000541902500054,
Author = {Jiang, R.},
Book-Group-Author = {IEEE},
Title = {A Novel Normalization Method for Using in Multiple Criteria Decision
   Analysis},
Booktitle = {2019 IEEE INTERNATIONAL CONFERENCE ON INDUSTRIAL ENGINEERING AND
   ENGINEERING MANAGEMENT (IEEM)},
