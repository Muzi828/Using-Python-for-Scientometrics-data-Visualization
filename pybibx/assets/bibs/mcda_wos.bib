Title = {A case-based model for sorting problems in multiple criteria decision
   analysis},
Booktitle = {INTERNATIONAL CONFERENCE ON SYSTEMS, MAN AND CYBERNETICS, VOL 1-4,
   PROCEEDINGS},
Series = {IEEE International Conference on Systems Man and Cybernetics Conference
   Proceedings},
Year = {2005},
Pages = {215-220},
Note = {IEEE International Conference on Systems, Man and Cybernetics, Waikoloa,
   HI, OCT 10-12, 2005},
Abstract = {Sorting is a process for multiple criteria decision analysis that
   arranges a set of alternatives into ordinally defined groups. Based on
   weighted Euclidean distance, a case-based distance model is proposed for
   sorting using weights and group thresholds obtained by assessment of a
   case set. A quadratic optimization program is employed to find optimal
   weights and group thresholds. Instead of direct inquiry, this case-based
   method can elicit the decision maker's preferences more expeditiously
   and accurately. A case study on Canadian municipal water usage analysis
   is presented.},
