@article{ WOS:000354288600003,
Author = {Chen, Ting-Yu},
Title = {IVIF-PROMETHEE outranking methods for multiple criteria decision
   analysis based on interval-valued intuitionistic fuzzy sets},
Journal = {FUZZY OPTIMIZATION AND DECISION MAKING},
