@article{ WOS:000360776800008,
Author = {Poplawska, Jolanta and Labib, Ashraf and Reed, Deborah M.},
Title = {A hybrid multiple-criteria decision analysis framework for corporate
   social responsibility implementation applied to an extractive industry
   case study},
Journal = {JOURNAL OF THE OPERATIONAL RESEARCH SOCIETY},
Year = {2015},
Volume = {66},
Number = {9},
Pages = {1491-1505},
Month = {SEP},
Abstract = {Integration of corporate social responsibility (CSR) into a company's
   mainstream strategy is a complex task. Practical implementation of CSR
   requires analysis of both the external and internal environments to
   determine the prospects and challenges significantly influencing
   integration of sustainability into business strategy. In order to
   overcome limitations of single multiple-criteria decision analysis
   models, this paper proposes a hybrid integrated framework combining
   cognitive mapping and analytic networks process (ANP) to determine,
   prioritise and select CSR programmes for implementation. The strategic
   cognitive map serves as a foundation to build the ANP network and
   identify the importance of CSR programmes. A knapsack optimisation
   method is then used to optimally assign resources to CSR alternatives.
   We demonstrate the usefulness of the framework through a case study in
   the extractive sector. The framework was empirically tested with 61
   respondents using postal and online surveys, MBA workshops, and
   conference networking.},
