@article{ WOS:000441488800008,
Author = {Kovaleva, Yulia and Ostadhassan, Mehdi and Tamimi, Naser and Kovalev,
   Alexey},
Title = {A preliminary optimization of borehole microseismic array design with a
   multiple criteria decision analysis},
Journal = {JOURNAL OF APPLIED GEOPHYSICS},
Year = {2018},
Volume = {157},
Pages = {87-95},
Month = {OCT},
Abstract = {Economic development of unconventional reservoirs requires a combination
   of different techniques such as horizontal drilling and hydraulic
   fracturing. To obtain valuable information about the effectiveness of a
   hydraulic fracturing operation, it is a common practice to use borehole
   microseismic monitoring by deploying arrays of geophones in wells
   nearby. Microseismic monitoring provides important data about fracture
   parameters including: half-length, geometry, orientation and complexity
   of the fracture network. Based on this information, stimulation programs
   can be improved to increase production volumes. However, inaccurate
   microseismic acquisition design can result in poor quality raw
   microseismic data that undermines the success of this important
   operation.
   In this study, we developed a comprehensive workflow using Multiple
   Criteria Decision Analysis (MCDA) to optimize microseismic acquisition
   design in order to improve borehole microseismic data quality. The
   influence of multiple parameters, such as average distance to
   microseismic events, receiver and event aperture, and wave field
   complexity, is considered. A computer code is developed to resolve the
   conflicting parameters to achieve the best possible data quality. The
   program output is used recommend an optimum depth and location for
   deploying the borehole geophone array(s). The algorithm is then applied
   to a case study that utilizes synthetic microseismic data and real well
   trajectories from several vertical and horizontal wells in Williston
   Basin, North Dakota. (C) 2018 Elsevier B.V. All rights reserved.},
