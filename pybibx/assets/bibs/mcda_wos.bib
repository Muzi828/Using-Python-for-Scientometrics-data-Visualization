@article{ WOS:000749712800001,
Author = {Hurajova, Jana Coronicova and Hajduova, Zuzana},
Title = {Multiple-Criteria Decision Analysis Using TOPSIS and WSA Method for
   Quality of Life: The Case of Slovakia Regions},
Journal = {MATHEMATICS},
Year = {2021},
Volume = {9},
Number = {19},
Month = {OCT},
Abstract = {Quality of life can be expressed as a highly subjective measure of
   happiness, which is an important part of many financial and
   non-financial decisions. The issue of quality of life, namely its
   quantification, is a current topic, evidenced by a number of literary
   sources in both foreign and domestic literature. The paper deals with
   this issue in the conditions of the Slovak Republic and studies the
   quality of life for each region of Slovakia with respect to the given
   indicators. In this paper, we deal with the use of the methods of
   multi-criteria analysis in evaluating the quality of life in Slovakia.
   The aim of this paper is to highlight that it is important to monitor
   the level of quality of life at the regional level and that one of the
   indicators influencing the quality of life in the region may be its
   location in relation to neighboring countries.},
