@article{Georgopoulou2003199,
 abbrev_source_title = {Eur J Oper Res},
 abstract = {The introduction of the Kyoto Protocol in 1997 represents a very important step in the effort for the limitation of the emissions of greenhouse gases. The energy sector, being responsible for the large majority of emissions, represents the most crucial field for undertaking mitigation actions. A large number of potential emission reduction measures exist in this sector, characterised by different cost, applicability difficulties, environmental and social impacts. These criteria define the degree of acceptance of the various actors involved in the decision-making process. In this paper, the possibilities offered by a multicriteria method - namely ELECTRE Tri - are investigated through its application in the case study of Greece, in order to define national priorities for a National Action Plan for GHG mitigation in the energy sector and to formulate a relevant time-schedule for actions' implementation. © 2002 Elsevier Science B.V. All rights reserved.},
