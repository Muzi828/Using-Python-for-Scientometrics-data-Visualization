 language = {English},
 note = {cited By 31},
 number = {5},
 pages = {567-581},
 source = {Scopus},
 title = {Multi–criteria Classification Methods in Financial and Banking Decisions},
 url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-26444532528&doi=10.1111%2f1475-3995.00374&partnerID=40&md5=8f28f5d7d8d7e29e17563a8cf2aa08cc},
