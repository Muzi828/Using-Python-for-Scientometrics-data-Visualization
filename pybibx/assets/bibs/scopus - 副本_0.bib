 journal = {Portugal SB 2007 - Sustainable Construction, Materials and Practices: Challenge of the Industry for the New Millennium},
 keywords = {Decision making;  Forestry;  Sustainable development, Aggregation methods;  Decision makers;  Decision process;  Integrated approach;  Integrated decision process;  Multi-criteria decision making methods;  Sustainable performance;  Tree structures, Trees (mathematics)},
 language = {English},
 note = {cited By 2; Conference of International Conference on Sustainable Construction, Materials and Practices: Challenge of the Industry for the New Millennium, Portugal SB 2007 ; Conference Date: 12 September 2007 Through 14 September 2007;  Conference Code:104441},
 pages = {418-425},
 publisher = {IOS Press},
 references = {Abernot, Y., (1996) Les Méthodes D'évaluation Scolaire, , Paris: Dunod; Adolphe, L., Sagacités: Vers un système à la gestion des ambiances urbaines (2002) Rapport Final, MENRT, DT no 99-V0532, 400p. , February 2002, Toulouse; Adolphe, L., Rousval, B., Beaumont, J., Maurin, M., Goger, T., Joumard, R., Projet PIE, L'aide à l'évaluation environnementale pour les projets d'aménagement: Applications à un système de transports: Propositions (2006) Rapport Final -Tome 2, Convention INRETS-EAT, 120p. , July 2006 Toulouse; Goger, T., (2004) Synthèse Bibliographique sur L'évaluation des Pollutions de L'air Générées par le Secteur des Transports: Application Au Cas de la Métropole Française, 64p. , June 2004, Bron; Keeney, R.L., (1992) Value-focused Thinking: A Path to Creative Decision Making, , Cambridge: Harvard University Press; Maurin, M., (2004) Problématique pour Une Approche Globale Multi-nuisances dans L'environnement, 16p. , Inrets - LTE June 2004, Bron; Rousval, B., (2005) Aide Multicritère À L'évaluation de L'impact des Transports sur L'environnement, , Thèse de doctorat, Université Paris IX-Dauphine, Lamsade, Paris; Roy, B., Bouyssou, D., (1993) Aide Multicritère À la Décision: Méthodes et Cas, 250p. , Université de Paris Dauphine, Lamsade, Paris: Economica; (1992) Conférence des Nations Unies sur L'Environnement et le Développement, , UNO Agenda 21, Rio de Janeiro; Yu, W., (1992) Aide Multicritère À la Décision dans le Cas de la Problématique du Tri: Concepts, Méthodes et Application, , Thèse de doctorat, Université Paris IX-Dauphine, Lamsade, Paris},
