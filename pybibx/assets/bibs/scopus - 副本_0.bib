 author_keywords = {Agroecological approach;  ELECTRE;  Multicriteria decision aid methods;  Pesticides;  Viticulture},
 correspondence_address1 = {Macary, F.; Inrae, France; email: francis.macary@inrae.fr},
 document_type = {Article},
