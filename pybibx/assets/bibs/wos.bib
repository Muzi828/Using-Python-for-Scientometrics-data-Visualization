@article{ WOS:000305663600007,
Author = {Rasool, Mehdizadeh and Franck, Taillandier and Denys, Breysse and
   Halidou, Niandou},
Title = {{Methodology and tools for risk evaluation in construction projects using
   Risk Breakdown Structure}},
Journal = {{EUROPEAN JOURNAL OF ENVIRONMENTAL AND CIVIL ENGINEERING}},
Year = {{2012}},
Volume = {{16}},
Number = {{1, SI}},
Pages = {{S78-S98}},
Month = {{JUN}},
Abstract = {{Risk Breakdown Structure (RBS) is a hierarchically organised depiction
   of the identified project risks arranged by risk category and
   subcategory that identifies the various areas and causes of potential
   risks. This type of representation has many advantages and is a suitable
   tool especially for risk management of construction projects, since it
   offers a synthetic view on risks, each stakeholder can have his or her
   own view on the project and it is compatible with evolutionary and
   dynamic nature of project risks. However, RBS suffers several drawbacks
   such as lack of consensus on how to develop an RBS for a new project,
   lack of clarity and inconsistencies in definition of risk categories and
   lack of rules enabling transfer of qualitative/quantitative information
   of risks across the tree. This article will focus on the last case,
   proposing an advanced method for measurement and propagation of risks in
   RBS.}},
