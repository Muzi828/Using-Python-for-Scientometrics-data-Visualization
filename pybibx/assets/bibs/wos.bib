@article{ WOS:000673206300001,
Author = {Wang, Lei and Wang, Xin and Ding, Zhaowei},
Title = {{Cask Principle of Multi-Attribute Risk Assessment: Non-Weighted Maximal
   Approach for Production Accidents}},
