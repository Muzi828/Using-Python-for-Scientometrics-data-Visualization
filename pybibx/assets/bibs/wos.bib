@article{ WOS:000259898100018,
Author = {Tervonen, Tommi and Figueira, Jose Rui and Lahdelma, Risto and Dias,
   Juscelino Almelda and Salminen, Pekka},
Title = {{A stochastic method for robustness analysis in sorting problems}},
Journal = {{EUROPEAN JOURNAL OF OPERATIONAL RESEARCH}},
Year = {{2009}},
Volume = {{192}},
Number = {{1}},
Pages = {{236-242}},
Month = {{JAN 1}},
Abstract = {{ELECTRE TRI is a multiple criteria decision aiding sorting method with a
   history of successful real-life applications. In ELECTRE TRI, values for
   certain parameters have to be provided. We propose a new method,
   SMAA-TRI, that is based oil stochastic multicriteria acceptability
   analysis (SMAA), for analyzing the stability of such parameters. The
   stability analysis can be used for deriving robust conclusions. SMAA-TRI
   allows ELECTRE TRI to be used with uncertain, arbitrarily distributed
   values for weights, the lambda cutting level, and profiles. The method
   consists of analyzing finite spaces of arbitrarily distributed parameter
   values. Monte Carlo simulation is applied in this in order to describe
   for each alternative the share of parameter values that have it assigned
   to different categories. We show the real-life applicability by
   re-analyzing a case Study in the field of risk assessment. (C) 2007
   Elsevier B.V. All rights reserved.}},
