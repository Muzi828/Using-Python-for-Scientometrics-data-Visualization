@inproceedings{ WOS:000332201900123,
Author = {Pereira, D. V. S. and Mota, C. M. M. and Miranda, J. A. G.},
Book-Group-Author = {{IEEE}},
Title = {{Municipal Human Development Index for brazilian cities based on ELECTRE
   TRI-C}},
Booktitle = {{2013 IEEE INTERNATIONAL CONFERENCE ON SYSTEMS, MAN, AND CYBERNETICS (SMC
   2013)}},
Series = {{IEEE International Conference on Systems Man and Cybernetics Conference
   Proceedings}},
Year = {{2013}},
Pages = {{736-740}},
Note = {{IEEE International Conference on Systems, Man, and Cybernetics (SMC),
   Manchester, ENGLAND, OCT 13-16, 2013}},
Abstract = {{The Human Development Index is an important indicator that evaluates
   well-being from the geometric mean of three dimensions: income, health
   and education. This indicator is often used to support decisions, but
   its relevance is intensified when measurements are taken at detailed
   levels. In this context, this study sets out to calculate the Municipal
   Human Development Index for 5,507 municipalities in Brazil. As a result,
   cities were classified into four development levels. Among Brazilian
   cities, 2.63\% were classified as having a very high human development;
   25.48\% as high; 33.14\% as medium; and 38.75\% as low. The
   classification of approximately 54\% of the municipalities was changed.
   The new approach enabled areas to be classified in a way that was more
   faithful to reality, without considering that half the population lives
   with a high quality of life. The multicriteria method ELECTRE TRI-C was
   used to mitigate some criticism of the index.}},
