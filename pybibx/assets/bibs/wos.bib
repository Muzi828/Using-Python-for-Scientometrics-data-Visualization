@article{ WOS:000528262900004,
Author = {Dehraj, Pooja and Sharma, Arun},
Title = {{Autonomic computing based trustworthiness attributes weight estimation
   using Electre-Tri method}},
Journal = {{JOURNAL OF INTERDISCIPLINARY MATHEMATICS}},
Year = {{2020}},
Volume = {{23}},
Number = {{1, SI}},
Pages = {{21-29}},
Abstract = {{With the advancement in autonomic computing technology, quality
   assurance for autonomic software is another big challenge to be faced by
   IT industries. Autonomic software is quite different in working compared
   to non-autonomic software. So, quality estimation parameters will also
   be different. Quality assurance method for autonomic software has not
   been standardized yet but quality parameters based on autonomic features
   have been suggested by researchers. Based on the functioning of
   autonomic software and autonomic capabilities, Autonomicity and
   Trustworthiness are considered to be the major quality estimation
   metrics. In this paper, trustworthiness estimation is performed using
   six attributes. To contribute in this area further, attributes are
   ranked based on the weights estimated using Electre-Tri multi-criteria
   outranking method. The attributes weights can be used further to
   estimate trustworthiness for any autonomic software. With this, the
   quality estimation task can also be processed for autonomic software.}},
