@inproceedings{ WOS:000278935200019,
Author = {Oliveira, Eunice and Antunes, Carlos Henggeler},
Editor = {{Cowling, P and Merz, P}},
Title = {{An Evolutionary Algorithm Guided by Preferences Elicited According to
   the ELECTRE TRI Method Principles}},
