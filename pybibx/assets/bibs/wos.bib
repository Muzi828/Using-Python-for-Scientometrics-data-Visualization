@article{ WOS:000501466500001,
Author = {Hamdani, Nesrine and Hamdadou, Djamila},
Title = {{A Multicriteria Group Decision Support System: An Approach Based Agents
   and Web Services}},
Journal = {{INTERNATIONAL JOURNAL OF INFORMATION TECHNOLOGY AND WEB ENGINEERING}},
