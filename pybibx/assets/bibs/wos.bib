@inproceedings{ WOS:000392426503044,
Author = {Mayag, B. and Cailloux, O. and Mousseau, V.},
Editor = {{Berenguer, C and Grall, A and Soares, CG}},
Title = {{MCDA tools and risk analysis: The decision deck project}},
